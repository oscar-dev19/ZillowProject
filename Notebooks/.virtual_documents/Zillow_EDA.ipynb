import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt

def plot_correlation_heatmap(df, figsize=(10, 10), cmap='coolwarm', title='Correlation Heatmap'):
    # Encode categorical data
    df_encoded = pd.get_dummies(df)

    correlation_matrix = df_encoded.corr()
    
    plt.figure(figsize=figsize)

    sns.heatmap(correlation_matrix, annot=True, cmap=cmap, center=0)

    plt.title(title)

    plt.show()



plot_correlation_heatmap(ca_num_listings_df, title="CA Listings Correlations")








